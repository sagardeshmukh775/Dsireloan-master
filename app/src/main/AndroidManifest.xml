<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smartloan.smtrick.dezire_loan_admin">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/dloan"
        android:label="@string/app_name"
        android:roundIcon="@drawable/dloan"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.smartloan.smtrick.dezire_loan_admin.view.activites.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.smartloan.smtrick.dezire_loan_admin.view.activites.LoginScreen"
            android:windowSoftInputMode="stateHidden" />

        <activity
            android:name="com.smartloan.smtrick.dezire_loan_admin.view.activites.Registeractivity"
            android:windowSoftInputMode="stateHidden" />

        <activity
            android:name="com.smartloan.smtrick.dezire_loan_admin.view.activites.MainActivity"
            android:theme="@style/AppTheme"
            android:windowSoftInputMode="stateHidden" />

        <activity
            android:name="com.smartloan.smtrick.dezire_loan_admin.view.activites.NewLeadActivity"
            android:theme="@style/AppTheme"
            android:windowSoftInputMode="stateHidden" />

        <activity
            android:name="com.smartloan.smtrick.dezire_loan_admin.view.activites.TermsCondition_Activity"
            android:theme="@style/AppTheme"
            android:windowSoftInputMode="stateHidden" />

        <activity
            android:name="com.smartloan.smtrick.dezire_loan_admin.view.activites.GenerateLeads"
            android:label="@string/title_activity_generate_leads"
            android:windowSoftInputMode="stateHidden" />

        <activity
            android:name="com.smartloan.smtrick.dezire_loan_admin.view.activites.ImageSwipZoomActivity"
            android:label="@string/title_activity_generate_leads"
            android:windowSoftInputMode="stateHidden" />

        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:screenOrientation="portrait" />

        <service
            android:name="com.smartloan.smtrick.dezire_loan_admin.service.UploadImageService"
            android:exported="false"
            android:stopWithTask="false" />

        <service
            android:name="com.smartloan.smtrick.dezire_loan_admin.service.ImageUploadIntentService"
            android:exported="false"
            android:stopWithTask="false" />

        <activity
            android:name="com.smartloan.smtrick.dezire_loan_admin.view.activites.UpdateProfileActivity"
            android:windowSoftInputMode="stateHidden" />

        <activity
            android:name="com.smartloan.smtrick.dezire_loan_admin.view.activites.Add_Updatelead_C_Details_Activity"
            android:windowSoftInputMode="stateHidden" />

        <service
            android:name="com.smartloan.smtrick.dezire_loan_admin.service.UploadImageService"
            android:exported="false"
            android:stopWithTask="false" />

        <service
            android:name="com.smartloan.smtrick.dezire_loan_admin.service.ImageUploadIntentService"
            android:exported="false"
            android:stopWithTask="false" />

        <activity
            android:name="com.smartloan.smtrick.dezire_loan_admin.view.activites.Add_Updatelead__bankresult_Activity"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.smartloan.smtrick.dezire_loan_admin.view.fragements.Add_Updatelead__approvedloan_Activity"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.smartloan.smtrick.dezire_loan_admin.view.activites.Add_Updatelead__submittobank_Activity"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.smartloan.smtrick.dezire_loan_admin.view.activites.ImagePickerActivity"
            android:label="Pick Image"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity
            android:name="com.smartloan.smtrick.dezire_loan_admin.view.activites.CropImageActivity"
            android:label="Crop Image"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity
            android:name="com.smartloan.smtrick.dezire_loan_admin.view.activites.Advertise_Activity"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.smartloan.smtrick.dezire_loan_admin.view.activites.Invoice_Details_Activity"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.smartloan.smtrick.dezire_loan_admin.view.activites.User_Invoice_Details_Activity"
            android:windowSoftInputMode="stateHidden" />

        <activity android:name="com.smartloan.smtrick.dezire_loan_admin.view.activites.Phone_Verification_Activity"/>
        <activity android:name="com.smartloan.smtrick.dezire_loan_admin.view.activites.VerifyPhoneActivity"/>
    </application>

</manifest>